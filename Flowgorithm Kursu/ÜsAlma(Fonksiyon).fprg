    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="us, taban, sonuc" type="Integer" array="False" size=""/>
            <output expression="&quot;&#220;sl&#252; say&#305;n&#305;z&#305;n taban&#305;n&#305; giriniz.&quot;" newline="True"/>
            <input variable="taban"/>
            <output expression="&quot;&#220;sl&#252; say&#305;n&#305;z&#305;n &#252;ss&#252;n&#252; giriniz.&quot;" newline="True"/>
            <input variable="us"/>
            <call expression="UsAl(us,taban)"/>
            <assign variable="sonuc" expression="UsAl(us,taban)"/>
            <output expression="&quot;Girdi&#287;iniz &#252;sl&#252; say&#305;n&#305;n sonucu: &quot; &amp; sonuc" newline="True"/>
