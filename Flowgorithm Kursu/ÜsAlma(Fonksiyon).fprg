    <function name="UsAl" type="Integer" variable="sonuc">
        <parameters>
            <parameter name="us" type="Integer" array="False"/>
            <parameter name="taban" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="sonuc" type="Integer" array="False" size=""/>
            <assign variable="sonuc" expression="1"/>
            <while expression="us != 0">
                <assign variable="sonuc" expression="sonuc * taban"/>
                <assign variable="us" expression="us - 1"/>
            </while>
        </body>
    </function>
