        </body>
    </function>
    <function name="faktoriyelal" type="Integer" variable="sonuc">
        <parameters>
            <parameter name="sayi" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="i, sonuc" type="Integer" array="False" size=""/>
            <assign variable="sonuc" expression="1"/>
            <for variable="i" start="1" end="10" direction="inc" step="1">
                <assign variable="sonuc" expression="sonuc  * i"/>
            </for>
        </body>
    </function>
